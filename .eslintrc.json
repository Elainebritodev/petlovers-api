{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi":"off",
        "no-console":"off",
        "quotes":"off",
        "linebreak-style":"off",
        "arrow-body-style": "off",
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off",
        "no-underscore-dangle": "off"
    }
}
